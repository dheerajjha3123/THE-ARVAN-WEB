packages:
  yum:
    git: []
commands:
  01_node_get:
    command: "curl -o- https://raw.githubusercontent.com/nvm-sh/nvm/v0.39.0/install.sh | bash"
  02_node_install:
    command: "source ~/.bashrc && nvm install 18 && nvm use 18"
  03_node_default:
    command: "source ~/.bashrc && nvm alias default 18"
